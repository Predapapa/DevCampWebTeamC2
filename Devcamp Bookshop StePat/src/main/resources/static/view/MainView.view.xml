<mvc:View controllerName="BWATC.BookstoreWebAppTC.controller.MainView" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">

		<App>
			<pages>
				<Page id="page" title="{i18n>title}" showNavButton="true" navButtonPress=".onNavBack">
					<content>
						<List
		items="{/}"
		headerText="Books">
		<ObjectListItem
			title="{title}"
			type="Active"
			icon="sap-icon://course-book"
			press=".onPress"
			>
			<firstStatus>
				<ObjectStatus
					text="Available"
					 />
			</firstStatus>
			<secondStatus>
				<ObjectStatus
					text="{price} EUR"
					/>
			</secondStatus>
			<ObjectAttribute text="from {author}"/>
			<!-- <ObjectAttribute text="isbn" text="{isbn}"/> -->
			<!-- <ObjectAttribute text="uuid" text="{uuid}"/> -->
		</ObjectListItem>
	</List>
					</content>
				</Page>
			</pages>
		</App>

</mvc:View>